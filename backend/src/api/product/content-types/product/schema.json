{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "proImg": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    },
    "proName": {
      "type": "string",
      "required": true
    },
    "proPrice": {
      "type": "decimal",
      "required": true
    },
    "proInfo": {
      "type": "text",
      "required": true
    },
    "proRating": {
      "type": "decimal",
      "required": true
    },
    "proGroup": {
      "type": "enumeration",
      "enum": [
        "men",
        "women"
      ],
      "required": true
    }
  }
}
